  <script>
    var current_path = window.location.pathname;
    function setClass(id) {
      document.getElementById(id).setAttribute("class", "nav-item active");
    }
    switch (current_path) {
      case "/":
        setClass("navHome");
        break;
      case "/maps/view":
        setClass("navMap");
        break;
      case "/chart/view":
        setClass("navChart");
        break;
      case "/auth/password/chpw":
        setClass("navReset");
        break;
      case "/auth/cancel/cancellation":
        setClass("navCancel");
        break;
      default:
        break;
    }
  </script>

